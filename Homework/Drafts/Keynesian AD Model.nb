    3.770669404585203*^9, 3.7706694591298933`*^9}, {3.7706695114016333`*^9, 
    3.7706695653012066`*^9}, {3.770669620100222*^9, 3.7706696204439497`*^9}, {
    3.7706700678067226`*^9, 3.7706700853092513`*^9}, {3.770670157684041*^9, 
    3.770670212400231*^9}, {3.770670248727559*^9, 3.7706702637842145`*^9}, 
    3.7706703007165613`*^9},ExpressionUUID->
  "3804e4b0-3b9c-41df-9c1d-fea6075881d4"],
